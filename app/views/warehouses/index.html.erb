<div class="container">
  <h2>Avaliable Warehouses</h2>
</div>

<div class="info container">
  <div class="row">
    <% @warehouses.each do |d| %>
      <div class="col-md-4">
        <h5>Location: <%= d.location %></h5>
        <p>Capacity: <%= d.capacity %></p>
        <p>Avaliability: <%= d.avaliability %></p>
        <p>Funding Required: <%= d.funding %></p>
      </div>
    <% end %>
  </div>
</div>